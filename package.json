{
    "name": "osbb-frontend",
    "version": "1.0.0",
    "author": "hydrophonixfx@gmail.com",
    "private": true,
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/webpack/start.js\"",
        "build": "cross-env-shell NODE_ENV=production node --require @babel/register \"./scripts/webpack/build.js\"",
        "analyze": "webpack-bundle-analyzer \"./build/stats.json\"",
        "serve": "serve \"./build\" -s",
        "gen": "graphql-codegen --config graphql-codegen.yml",
        "apollo:schema": "apollo service:download graphqlSchema.json",
        "apollo:gen": "apollo codegen:generate --localSchemaFile=graphqlSchema.json --target=typescript --globalTypesFile=src/@types/graphql-global-types.ts ./types"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
